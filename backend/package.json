{
  "name": "readingly-api",
  "version": "0.1.0",
  "description": "The API for Readingly, a reading tracker to help you read better",
  "main": "index.js",
  "engines": {
    "node": "12.18.2"
  },
  "scripts": {
    "test": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
