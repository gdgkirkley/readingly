    "express-jwt": "^6.0.0",
    "graphql": "^15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-scalars": "^1.2.2",
    "graphql-shield": "^7.3.2",
    "graphql-tools": "^6.0.14",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.6.8",
    "pg": "^8.3.0",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "object-hash": "^2.0.3",
    "prettier": "^2.0.5"
